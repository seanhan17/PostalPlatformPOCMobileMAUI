<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="RoyalMailPOC.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:RoyalMailPOC"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="Camera">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate views:Camera}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Map">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate views:Map}" />
        </Tab>
    </FlyoutItem>
    <!--<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->

</Shell>
